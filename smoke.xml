<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Tutorialsninja Automation Suite">
	<listeners>
		<listener class-name="com.tutorialsninja.qa.listeners.MyListeners"></listener>
	</listeners>
	<test name="smoke Test cases">
		<groups>
			<run>
				<include name="smoke"></include>
			</run>
		</groups>
		<classes>
			<class name="com.tutorialsninja.qa.testCases.TestRegister" />
			<class name="com.tutorialsninja.qa.testCases.TestLogin" />
			<class name="com.tutorialsninja.qa.testCases.TestLogout" />
			<class name="com.tutorialsninja.qa.testCases.TestForgetPassword" />		
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
